{
  "name": "@solid-primitives/scheduled",
  "version": "1.3.2",
  "description": "Primitives for creating scheduled — throttled or debounced — callbacks.",
  "contributors": [
    "David Di Biase <dave.dibiase@gmail.com>",
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/scheduled#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "scheduled",
    "stage": 3,
    "list": [
      "debounce",
      "throttle",
      "scheduleIdle",
      "leading",
      "createScheduled"
    ],
    "category": "Utilities"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "keywords": [
    "solid",
    "primitives",
    "scheduled",
    "debounce",
    "throttle"
  ],
  "devDependencies": {
    "@solid-primitives/timer": "^1.3.7",
    "@solidjs/router": "^0.8.0"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {},
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  }
}