{
  "name": "@solid-primitives/styles",
  "version": "0.0.108",
  "description": "Collection of reactive primitives focused on styles.",
  "author": "Damian Tarnawski <gthetaranv@gmail.com>",
  "contributors": [
    "Tom Pichaud <dev.tompichaud@icloud.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/styles#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "styles",
    "stage": 0,
    "list": [
      "createRemSize"
    ],
    "category": "Display & Media"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "keywords": [
    "solid",
    "primitives",
    "styles",
    "css"
  ],
  "dependencies": {
    "@solid-primitives/rootless": "^1.3.2",
    "@solid-primitives/utils": "^6.0.0"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {},
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  }
}